{{page-title "Locations"}}

<AppointmentContainer @step="2">
  <ul>
    {{#each @model as |location|}}
      <li>
        {{#if location.isVideo}}
          👨🏻‍💻
        {{else}}
          🗺
        {{/if}}
        <hr />
        <h4>{{location.name}}</h4>
        <h5>{{location.phone}}</h5>
        <button {{on "click" this.select}}>Select</button>
      </li>
    {{/each}}
  </ul>
</AppointmentContainer>
